{
  "name": "loco-por-la-cocina",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev & npx next-video sync -w",
    "dev:test": "NEXT_PUBLIC_OPERATOR_COUNTRY=test next dev -p 3503",
    "dev:temcel-mozambique": "NEXT_PUBLIC_OPERATOR_COUNTRY=temcel-mozambique next dev -p 3504",
    "build": "next build",
    "build:test": "NEXT_PUBLIC_OPERATOR_COUNTRY=test NEXT_DIST_DIR=.next-test next build",    
    "build:temcel-mozambique": "NEXT_PUBLIC_OPERATOR_COUNTRY=temcel-mozambique NEXT_DIST_DIR=.next-temcel-mozambique next build",
    "build:prod": "pnpm run build:temcel-mozambique",
    "build:all": "pnpm run build:test && pnpm run build:temcel-mozambique",
    "start": "next start",
    "start:test": "NEXT_PUBLIC_OPERATOR_COUNTRY=test NEXT_DIST_DIR=.next-test next start -p 3503",    
    "start:temcel-mozambique": "NEXT_PUBLIC_OPERATOR_COUNTRY=temcel-mozambique next start -p 3504",    
    "lint": "eslint .",
    "prepare": "husky",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.15",
    "@u-wave/react-vimeo": "^0.9.11",
    "axios": "^1.7.9",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lottie-react": "^2.4.0",
    "lucide-react": "^0.460.0",
    "motion": "^11.18.0",
    "next": "15.0.2",
    "next-crypto": "^1.0.8",
    "next-video": "^1.3.3",
    "node-html-parser": "^6.1.13",
    "react": "19.0.0-rc-02c0e824-20241028",
    "react-dom": "19.0.0-rc-02c0e824-20241028",
    "react-player": "^2.16.0",
    "react-scroll-motion": "^0.3.4",
    "react-share": "^5.1.2",
    "react-shared": "^1.1.1",
    "react-window": "^1.8.10",
    "swiper": "^11.1.15",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.0.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
